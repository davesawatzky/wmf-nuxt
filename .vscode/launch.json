{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      // Change to use npm run dev so we don't execute shell stub directly
      "type": "node",
      "request": "launch",
      "name": "Debug Nuxt",
      "outputCapture": "std",
      "program": "${workspaceFolder}/node_modules/nuxt/bin/nuxt.mjs",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["run", "debug"],
      "console": "integratedTerminal"
      // "envFile": "${workspaceFolder}/.env.development",
      // "outFiles": [
      //   "${workspaceFolder}/.nuxt/dist/**/*.(m|c|)js",
      //   "!**/node_modules/**"
      // ]
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Debug Nuxt (client)",
      "url": "http://localhost:3001",
      "webRoot": "${workspaceFolder}"

      // "sourceMapPathOverrides": {
      //   "webpack:///./*": "${workspaceFolder}/*",
      //   "webpack://_virtual_/*": "${workspaceFolder}/.nuxt/*",
      //   "vite:///*": "${workspaceFolder}/*",
      //   "vite:///src/*": "${workspaceFolder}/app/*"
      // }
    },
    {
      // Attach to browser if already open
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome",
      "url": "http://localhost:3001",
      "webRoot": "${workspaceFolder}"
      // "sourceMapPathOverrides": {
      //   "webpack:///./*": "${workspaceFolder}/*",
      //   "webpack://_virtual_/*": "${workspaceFolder}/.nuxt/*",
      //   "vite:///*": "${workspaceFolder}/*",
      //   "vite:///src/*": "${workspaceFolder}/app/*"
      // }
    }
  ],
  "compounds": [
    {
      "name": "Fullstack Debug",
      "configurations": ["Debug Nuxt", "Debug Nuxt (client)"]
    }
  ]
}
